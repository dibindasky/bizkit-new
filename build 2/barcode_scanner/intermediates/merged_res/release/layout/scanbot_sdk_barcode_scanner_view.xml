<?xml version="1.0" encoding="utf-8"?>
<io.scanbot.sdk.ui.camera.ScanbotCameraContainerView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scanbot_camera_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <io.scanbot.sdk.barcode.ui.BarcodePolygonsView
        android:id="@+id/barcodes_polygon_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <io.scanbot.sdk.ui.camera.FinderOverlayView
        android:id="@+id/finder_overlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:fixed_width="300dp"
        app:fixed_height="300dp">
    </io.scanbot.sdk.ui.camera.FinderOverlayView>

</io.scanbot.sdk.ui.camera.ScanbotCameraContainerView>
